.search-container{display:flex;flex-direction:row;align-items:center;margin:10rpx 20rpx 0}.search-container .search-content{box-sizing:border-box;display:flex;flex-direction:row;align-items:center;height:76rpx;line-height:76rpx;width:632rpx;padding:0 20rpx;border:1px solid #e9e9e9;border-radius:76rpx}.search-container .ver{background-color:#e1e1e1;height:40rpx;width:2px}.search-container .search-input{margin-left:12rpx;width:100%;font-size:28rpx;color:#353535}.search-container .placeholder{letter-spacing:2rpx;font-size:24rpx;color:#888}.search-container .cancel-img{display:inline-flex;align-self:center;margin-left:14rpx}.search-container .city-cascade{width:156rpx;display:flex;flex-direction:row;align-items:center;justify-content:space-around}.search-container .city-cascade text{font-size:24rpx;color:#888}.search-container .city-cascade .arrow-img{width:16rpx;height:10rpx}.search-container .city-transparent{width:16rpx}.search-container .picker{text-align:center;font-size:24rpx;color:#888}.search-container .btn{margin-left:20rpx;font-size:28rpx;color:#888}
