
  <view class="personal-container">
      <view class="personal-info" data-url="base_edit" bindtap="jumpto">
        <image mode="aspectFill" src="{{info.headimg?info.headimg:'/images/icons/upload_head.png'}}"/>
        <view class="name-intro"  wx:if="{{token}}">
          <view class="name">{{info.username!=''?info.username:info.recomphonenumber}}</view>
        </view>
        <view class="name-intro"  wx:else>
          <view class="intro2"><text>登录</text>/<text>注册</text></view>
        </view>
      </view>

      <view class="my-function">
        <view class="line bb flex alignitems" data-url="resume_list" bindtap="jumpto">
          <image class="icon" src="/images/icons/resume.png"/>
          <text class="flex1">我的简历</text>

          <view class="arrow">
            <image src="/images/icons/arrow_right.png"/>
          </view>
      </view>
      <view class="line bb flex alignitems" data-url="delivery" bindtap="jumpto">
        <image class="icon" src="/images/icons/delivery.png"/>
        <text class="flex1">投递状态</text>
          <view class="arrow">

              <image src="/images/icons/arrow_right.png"/>
          </view>
      </view>
      <view class="line bb flex alignitems" data-url="collection" bindtap="jumpto">
        <image class="icon" src="/images/icons/collection.png"/>
        <text class="flex1">我的收藏</text>
        <view class="arrow">
          <image src="/images/icons/arrow_right.png"/>
        </view>
      </view>
      <view class="line bb flex alignitems" bindtap="switchapplet">
        <image class="icon" src="/images/icons/switch_icon_qiye.png"/>
        <text class="flex1">切换到企业版</text>

        <view class="arrow">
            <image src="/images/icons/arrow_right.png"/>
        </view>
      </view>
    </view>
    <view class="exit" bindtap="logout" wx:if="{{token}}">退出登录</view>

    <!-- 退出弹框 S -->
    <view class="alert-container" wx:if="{{showbox}}"/>
    <view class="alert-box" wx:if="{{showbox}}">
        <view class="sure">确认退出登录？</view>
        <view class="yesno">
            <view class="no" bindtap="cancel">取消</view>
            <view class="yes" bindtap="yes">确定</view>
        </view>
    </view>
  </view>