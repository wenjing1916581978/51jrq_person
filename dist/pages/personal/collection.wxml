
    <view/>
        <block  wx:if="{{list.length}}">
          <block wx:for="{{list}}" wx:for-index="index" wx:for-item="item" wx:key="index">
              
  <view class="posi-corp-container" bindtap="$commposi$goDetails" data-com-index="{{index}}">
    <view class="posi-container">
        <view class="posi-name-sal">
            <text class="posi-name">{{item.jobname}}</text>
            <text class="posi-sal">{{item.salary}}</text>
        </view>
        <view class="posi-duty">
            <text>{{item.jobcity}}</text>
            <text class="ver"  wx:if="{{item.workyears}}">|</text>
            <text>{{item.workyears}}</text>
            <text class="ver"  wx:if="{{item.ebid}}">|</text>
            <text>{{item.ebid}}</text>
        </view>
    </view>
      <view class="corp-brief flex">
          <view class="logo">
              <image mode="aspectFit" src="{{item.mlogo == 'http://m.51jrq.com/null' ? 'https://www.51jrq.com/topics/images/51jrq_logo_gray.png' : item.mlogo}}"/>
          </view>
          <view class="corp-info flex1">
              <text class="corp-name ellipsis">{{item.corpname}}</text>
              <view class="corp-size-type">
                  <view class="corp-size">
                    <text>{{item.industry?item.industry:'暂无'}}</text>
                  </view>
                  <text class="up-date">{{item.refreshdate ? item.refreshdate : item.updatedate}}</text>
              </view>
          </view>
      </view>
  </view>

          </block>
        </block>
        <view class="no-collect-container"  wx:else/>
            <image src="/images/icons_collection.png"/>
            <view/>{{warningWord}}<iew/>
        <iew/>
    <iew/>