
  <view class="container">
    <!-- 基本信息 S -->
    <view class="flex alignitems baseinfo bb pb">
      <view bindtap="edititem" data-url="/pages/personal/base_info?resumeid={{resumeid}}" class="flex1">
        <view class="flex alignitems">
          <text>{{baseInfo.username}}</text>
        </view>
        <view class="baseinfo_attr flex alignitems"  wx:if="{{edit}}">
          <view class="editbase"><image src="../../images/icons/edit.png"/></view>
          <text>我的基本信息</text>
        </view>
        <view  wx:else>
          <view class="baseattr">{{baseInfo.address}} <text>|</text>  {{baseInfo.workyears}} <text>|</text>  {{baseInfo.educationbg}} <text>|</text>  {{baseInfo.age}}</view>
          <view class="baseattr">{{baseInfo.jobstatus}}</view>
        </view>
      </view>
      <view class="baseinfo_img">
        <view class="head">
          <image mode="aspectFill" src="{{baseInfo.headimg}}" wx:if="{{baseInfo.headimg}}"/>
          <image mode="aspectFill" src="/images/icons/upload_head.png" wx:else/>
        </view>
        <view class="sex">
          <image src="/images/icons/male.png" wx:if="{{sex}}"/>
          <image src="/images/icons/female.png" wx:else/>
        </view>
      </view>
    </view>
    <!-- 基本信息 E -->

    <!-- 附件 S -->
    <navigator url="/pages/personal/annex?resumeid={{resumeid}}&name={{baseInfo.fileshowname?baseInfo.fileshowname:''}}&time={{baseInfo.filectime?baseInfo.filectime:''}}" hover-class="none" class="bb flex alignitems annex" wx:if="{{edit}}">
        <image src="/images/icons/annex.png" class="annex_img"/>
        <view class="annex_txt flex1">
          <view class="ellipsis">{{baseInfo.fileshowname?baseInfo.fileshowname:'上传附件简历'}}</view>
        </view>
        <view class="item_edit" wx:if="{{edit}}">
          <image src="../../images/icons/arrow_right.png"/>
        </view>
    </navigator>
    <!-- 附件 E -->

    <!-- 联系方式 S -->
    <view class="bb pb" wx:else>
      <view class="title flex alignitems">
        <text>联系方式</text>
        <navigator wx:if="{{edit}}" hover-class="none" class="toedit" url="/pages/personal/job_want?resumeid={{resumeid}}&type=2"><image src="../../images/icons/edit.png"/></navigator>
      </view>
      <view class="content">
        <view class="desc">
          <view>手机：{{baseInfo.telephone}}</view>
          <view>邮箱：{{baseInfo.email}}</view>
        </view>

      </view>
    </view>
    <!-- 联系方式 E -->

    <!-- 自我评价 S -->
    <view class="bb pb">
      <view class="title flex alignitems">
        <text>自我评价</text>
        <navigator wx:if="{{edit}}" hover-class="none" class="toedit" url="/pages/personal/job_want?resumeid={{resumeid}}&type=2"><image src="../../images/icons/edit.png"/></navigator>
      </view>
      <view class="content" wx:if="{{jobApply.selfremark}}">
        <view class="desc">
          <text>{{jobApply.selfremark}}</text>
        </view>
      </view>
    </view>
    <!-- 自我评价 E -->

    <!-- 求职意向 S-->
    <view class="bb pb">
      <view class="title flex alignitems">
        <view class="flex1">求职意向</view>
        <navigator wx:if="{{edit}}" hover-class="none" class="toedit" url="/pages/personal/job_want?resumeid={{resumeid}}&type=1&functions={{jobApply.functions?jobApply.functions:''}}"><image src="../../images/icons/edit.png"/></navigator>
      </view>
      <view class="content" wx:if="{{jobApply.functions}}">
        <view class="item">
          <view class="item_tit">
            {{jobApply.functions}}
            <text wx:if="{{jobApply.functions}}">|</text> {{jobApply.postids}}
          </view>
          <view class="item_time">
            {{jobApply.jobnature}}
            <text wx:if="{{jobApply.jobnature}}">/</text>
            {{jobApply.sitecity}}
            <text wx:if="{{jobApply.sitecity}}">/</text>
            {{jobApply.expectsalarycode}}
            <text wx:if="{{jobApply.expectsalarycode}}">/</text>
            {{jobApply.poststime}}
          </view>
        </view>
      </view>

    </view>
    <!-- 求职意向 E -->

    <!-- 工作经历 S-->
    <view class="bb pb">
      <view class="title flex alignitems">
        <view class="flex1">工作经历</view>
        <navigator wx:if="{{edit}}" hover-class="none" class="newadd" url="/pages/personal/work_exper?resumeid={{resumeid}}">+</navigator>
      </view>
      <view class="content" wx:if="{{workExper.length}}">
        <block wx:for="{{workExper}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <view class="item">
            <view bindtap="edititem" class="flex alignitems" data-url="/pages/personal/work_exper?workid={{item.workid}}&resumeid={{resumeid}}">
              <view class="item_tit ellipsis flex1"> {{item.corpname}} </view>
              <view class="item_time"> {{item.starttime}} - {{item.endtime}} </view>
              <view class="item_edit" wx:if="{{edit}}">
                <image src="../../images/icons/arrow_right.png"/>
              </view>
            </view>
            <view class="item_post">{{item.postcode}}</view>
            <view class="desc">
              <text>{{item.workremark}}</text>
            </view>
          </view>
        </block>
      </view>
    </view>
    <!-- 工作经历 E-->

    <!-- 教育经历 S-->
    <view class="bb pb">
      <view class="title flex alignitems">
        <view class="flex1">教育经历</view>
        <navigator wx:if="{{edit}}" hover-class="none" url="/pages/personal/edu_exper?resumeid={{resumeid}}" class="newadd">+</navigator>
      </view>
      <view class="content" wx:if="{{eduExper.length}}">
        <block wx:for="{{eduExper}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <view bindtap="edititem" data-url="/pages/personal/edu_exper?educationid={{item.educationid}}&resumeid={{resumeid}}" class="item">
            <view class="flex alignitems">
              <view class="item_tit flex1 ellipsis"> {{item.school}} </view>
              <view class="item_time"> {{item.starttime}} - {{item.endtime}} </view>
              <view class="item_edit" wx:if="{{edit}}">
                <image src="../../images/icons/arrow_right.png"/>
              </view>
            </view>
            <view class="item_post">
              <view>{{item.specialtyid}} | {{item.educationbg}}</view>
            </view>
          </view>
        </block>
      </view>

    </view>
    <!-- 教育经历 E-->

    <!-- 项目经验 S-->
    <view class="bb pb">
      <view class="title flex alignitems">
        <view class="flex1">项目经验</view>
        <navigator wx:if="{{edit}}" hover-class="none" class="newadd" url="/pages/personal/project_exper?resumeid={{resumeid}}">+</navigator>
      </view>
      <view class="content" wx:if="{{projectExper.length}}">
        <block wx:for="{{projectExper}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <view bindtap="edititem" data-url="/pages/personal/project_exper?projectid={{item.projectid}}&resumeid={{resumeid}}" class="item">
            <view class="flex alignitems">
              <view class="item_tit flex1 ellipsis"> {{item.projectname}} </view>
              <view class="item_time"> {{item.starttime}} - {{item.endtime}} </view>
              <view class="item_edit" wx:if="{{edit}}">
                <image src="../../images/icons/arrow_right.png"/>
              </view>
            </view>
            <view class="desc">
              <text>{{item.projectremark}}</text>
            </view>
          </view>
        </block>
      </view>

    </view>
    <!-- 项目经验 E-->

    <!-- 证书 S-->
    <view class="pb">
      <view class="title flex alignitems">
        <view class="flex1">证书</view>
        <navigator wx:if="{{edit}}" hover-class="none" class="newadd" url="/pages/personal/certificate?resumeid={{resumeid}}">+</navigator>
      </view>
      <view class="content" wx:if="{{certificate.length}}">
        <block wx:for="{{certificate}}" wx:for-index="index" wx:for-item="item" wx:key="index">
          <view bindtap="edititem" data-url="/pages/personal/certificate?certid={{item.certid}}&resumeid={{resumeid}}" class="item">
            <view class="flex alignitems">
              <view class="item_tit flex1 ellipsis"> {{item.certname}} </view>
              <view class="item_time">{{item.gaintime}} 颁发</view>
              <view class="item_edit" wx:if="{{edit}}">
                <image src="../../images/icons/arrow_right.png"/>
              </view>
            </view>
          </view>
        </block>
      </view>
    </view>
    <!-- 证书 E-->

  </view>